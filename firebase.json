{
  "hosting": {
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "woonplan-hub-web",
          "region": "europe-west4"
        }
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "Dockerfile",
      "package-lock.json",
      "server.js",
      "index.tsx",
      "script.js",
      "index.css",
      "metadata.json"
    ]
  }
}
